<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/signin_googlesignin_button"
        style="@style/SignInButtonStyle"
        android:drawableStart="@drawable/ic_google_icon"
        android:text="@string/sign_in_with_google"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.919" />

    <ImageView
        android:id="@+id/signin_googlesignin_img"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:contentDescription="@string/sign_in_with_google"
        app:layout_constraintBottom_toBottomOf="@+id/signin_googlesignin_button"
        app:layout_constraintEnd_toEndOf="@+id/signin_googlesignin_button"
        app:layout_constraintHorizontal_bias="0.136"
        app:layout_constraintStart_toStartOf="@+id/signin_googlesignin_button"
        app:layout_constraintTop_toTopOf="@+id/signin_googlesignin_button"
        app:srcCompat="@drawable/ic_google_icon" />


</androidx.constraintlayout.widget.ConstraintLayout>